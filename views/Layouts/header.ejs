<body>
    <!--  -->
    <% if(visit_booked.length!=0) { %>
        <template id="visitBookedAlert">
        <!--  -->
        <swal-title>
            <%= visit_booked %>
        </swal-title>
        <!--  -->
        <swal-icon type="success"></swal-icon>
        <!--  -->
        <swal-button type="confirm">
            Ok
        </swal-button>
        <!--  -->
        <swal-param name="allowEscapeKey" value="false" />
        <!--  -->
        <swal-param
            name="customClass"
            value='{ "popup": "my-popup" }' />
        </template>
    <% } %>

    <!--  -->
    <div class="modal fade" id="modal" tabindex="-1" aria-labelledby="modalLabel" aria-hidden="true">
        <div class="modal-dialog">
          <div class="modal-content p-1">
            <div class="modal-header">
              <h2 style="color: #ff236c; font-family: Bubblegum Sans, sans-serif;" class="modal-title text-xl md:text-2xl" id="modalLabel">Book a Visit</h2>
              <button type="button" class="btn-close text-3xl text-slate-500" data-bs-dismiss="modal" aria-label="Close">&times;</button>
            </div>
            <div class="modal-body">
                <form action="/book-a-visit" method="post">
                    <div class="grid gap-4 md:grid-cols-2 mb-4 mt-1">
                        <input type="hidden" name="myUrl" id="myUrl">
                        <div>
                            <label for="parentNam" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Parent Name <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <input style="outline-color: #ff236c;" type="text" id="parentNam" name="parentName" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5" placeholder="Parent Name">
                            <div id="parent-name-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Parent Name is required</div>
                        </div>
                        <div>
                            <label for="childNam" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Child Name <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <input style="outline-color: #ff236c;" type="text" id="childNam" name="childName" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5" placeholder="Child Name">
                            <div id="child-name-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Child Name is required</div>
                        </div>
                        <div>
                            <label for="contNum" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Contact Number <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <input style="outline-color: #ff236c;" type="number" id="contNum" name="contactNumber" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5" placeholder="Contact Number">
                            <div id="cont-num-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Contact Number is required</div>
                            <div id="cont-num-format" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Invalid Contact Number</div>
                            <div id="cont-num-start" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Contact Number should not start with 0</div>
                        </div>
                        <div>
                            <label for="mailId" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Email <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <input style="outline-color: #ff236c;" type="text" id="mailId" name="email" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5" placeholder="Email">
                            <div id="mail-id-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Email is required</div>
                            <div id="mail-id-format" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Invalid Email</div>
                        </div>  
                        <div>
                            <label for="dat" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Date <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <input style="outline-color: #ff236c;" type="date" id="dat" name="date" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5">
                            <div id="dat-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Date cannot be blank </div>
                            <div id="dat-now" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Sorry, But you cannot book slot for today now</div>
                            <div id="dat-off" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Sorry, But we are closed on Sundays</div>
                        </div>
                        <div>
                            <label for="time" class="block mb-[2px] md:mb-1 text-sm md:text-base font-medium text-gray-900">Time Slot <sup class="text-red-500 text-sm md:text-base">*</sup></label>
                            <select style="outline-color: #ff236c;" id="time" name="timeSlot" class="text-sm md:text-base bg-gray-50 border border-gray-300 text-gray-900 rounded-lg block w-full p-2.5">
                                <option value="">--- Select Time Slot ---</option>
                                <option value="8am-11pm">8 am to 11 pm</option>
                                <option value="12pm-2pm">12 pm to 2 pm</option>
                            </select>
                            <div id="time-req" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Time Slot cannot be blank </div>
                            <div id="time-inv" class="text-sm md:text-base text-red-600 mt-[1px]" style="display: none;">Sorry, But you cannot book this time slot for today now </div>
                        </div>  
                      </div>
                      <button style="background-color: #ff236c;" class="my-submit-button text-white px-3 py-1 rounded-md text-sm md:text-base" id="bookAVisit" type="submit">Submit</button>
                </form>
            </div>
          </div>
        </div>
    </div>

