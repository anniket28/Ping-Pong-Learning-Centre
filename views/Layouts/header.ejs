    <script>
        // 
        window.onload=()=>{
            var d = new Date();

            var s = d.getFullYear() + '-' + ('0' + (d.getMonth()+1)).slice(-2) + '-' + ('0' + d.getDate()).slice(-2);
        
            var node = document.getElementById('dat');
            if (node) {
                node.setAttribute('min', s);
            }
        }
        // 
        document.getElementById('myUrl').value=location.href
        function ValidateEmail(mail) {
            if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(mail))
            {
                return (true)
            }
            return (false)
        }
        document.getElementById('bookAVisit').addEventListener('click',(event)=>{
            if(document.getElementById('parentNam').value.length==0){
                document.getElementById('parent-name-req').style.display='block'
                event.preventDefault()
            }
            else if(document.getElementById('parentNam').value.length!=0){
                document.getElementById('parent-name-req').style.display='none'
            }

            if(document.getElementById('childNam').value.length==0){
                document.getElementById('child-name-req').style.display='block'
                event.preventDefault()
            }
            else if(document.getElementById('childNam').value.length!=0){
                document.getElementById('child-name-req').style.display='none'
            }

            if(document.getElementById('contNum').value.length==0){
                document.getElementById('cont-num-req').style.display='block'
                document.getElementById('cont-num-format').style.display='none'
                document.getElementById('cont-num-start').style.display='none'
                event.preventDefault()
            }
            else if(document.getElementById('contNum').value.length!=0){
                if(document.getElementById('contNum').value[0]=='0'){
                    document.getElementById('cont-num-req').style.display='none'
                    document.getElementById('cont-num-format').style.display='none'
                    document.getElementById('cont-num-start').style.display='block'
                    event.preventDefault()
                }
                else{
                    if(document.getElementById('contNum').value.length==10){
                        document.getElementById('cont-num-req').style.display='none'
                        document.getElementById('cont-num-format').style.display='none'
                        document.getElementById('cont-num-start').style.display='none'
                    }
                    else{
                        document.getElementById('cont-num-req').style.display='none'
                        document.getElementById('cont-num-format').style.display='block'
                        document.getElementById('cont-num-start').style.display='none'
                        event.preventDefault()
                    }
                }
            }

            if(document.getElementById('mailId').value.length==0){
                document.getElementById('mail-id-req').style.display='block'
                event.preventDefault()
            }
            else if(document.getElementById('mailId').value.length!=0){
                if(ValidateEmail(document.getElementById('mailId').value)){
                    document.getElementById('mail-id-req').style.display='none'
                    document.getElementById('mail-id-format').style.display='none'
                }
                else{
                    document.getElementById('mail-id-req').style.display='none'
                    document.getElementById('mail-id-format').style.display='block'
                    event.preventDefault()
                }
            }

            if(document.getElementById('dat').value.length==0){
                document.getElementById('dat-req').style.display='block'
                document.getElementById('dat-now').style.display='none'
                document.getElementById('dat-off').style.display='none'
                event.preventDefault()
            }
            else if(document.getElementById('dat').value.length!=0){
                dt=new Date()
                var s = dt.getFullYear() + '-' + 
                    ('0' + (dt.getMonth()+1)).slice(-2) + '-' +
                    ('0' + dt.getDate()).slice(-2);
                s.toString()
                d=new Date(document.getElementById('dat').value)
                if(d.getDay()==0){
                    document.getElementById('dat-req').style.display='none'
                    document.getElementById('dat-off').style.display='block'
                    document.getElementById('dat-now').style.display='none'
                    event.preventDefault()
                }
                else if(dt.getHours()>14 && document.getElementById('dat').value==s){
                    document.getElementById('dat-req').style.display='none'
                    document.getElementById('dat-off').style.display='none'
                    document.getElementById('dat-now').style.display='block'
                    event.preventDefault()
                }
                else{
                    document.getElementById('dat-req').style.display='none'
                    document.getElementById('dat-off').style.display='none'
                    document.getElementById('dat-now').style.display='none'
                }
            }

            if(document.getElementById('time').value.length==0){
                document.getElementById('time-req').style.display='block'
                document.getElementById('time-inv').style.display='none' 
                event.preventDefault()
            }
            else if(document.getElementById('time').value.length!=0){
                dt=new Date()
                var s = dt.getFullYear() + '-' + 
                    ('0' + (dt.getMonth()+1)).slice(-2) + '-' +
                    ('0' + dt.getDate()).slice(-2);
                s.toString()
                if(document.getElementById('time').value==='8am-11pm' && dt.getHours()>11 && document.getElementById('dat').value==s){
                    document.getElementById('time-req').style.display='none'
                    document.getElementById('time-inv').style.display='block'
                    event.preventDefault()
                }
                else{
                    document.getElementById('time-req').style.display='none'
                    document.getElementById('time-inv').style.display='none'
                }
            }
        })

        // 
        if(document.getElementById('visitBookedAlert')){
            Swal.fire({
            template: '#visitBookedAlert'
            })
        }
    </script>
</body>